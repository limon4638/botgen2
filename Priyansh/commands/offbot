module.exports.config = {
  name: "offboot",
  version: "1.0",
  hasPermssion: 2,
  credits: "Kawsar",
  description: "Shut down bot safely",
  commandCategory: "System",
  usages: "",
  cooldowns: 0
};

module.exports.run = async function({ api, event }) {
  api.sendMessage("Bot is turning off safely...", event.threadID, () => {
    process.exit(0);
  });
}
